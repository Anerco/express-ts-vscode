{
  "name": "express-ts-vscode",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "// ------ TESTING ----- //": "",
    "test": "ts-mocha -p tsconfig.json src/**/*.test.ts",
    "// ------ BUILDING ----- //": "",
    "build": "tsc",
    "build:watch": "tsc -w",
    "// ------ DEV ----- //": "",
    "dev": "ts-node-dev --inspect --no-notify --respawn --transpile-only ./src/bin/www.ts",
    "// ------ DEBUGING ----- //": "",
    "docker-debug": "docker-compose -f dc-dev.yml up",
    "docker-debug-build": "docker-compose -f dc-dev.yml up --build",
    "// ------ LINTING ----- //": "",
    "lint:list": "eslint */**/*.js --fix",
    "lint": "eslint */**/*.js --fix",
    "// ------ NPM ----- //": "",
    "prepublishOnly": "tsc",
    "// ------ START ----- //": "",
    "start": "ts-node-dev --max-old-space-size=8192 --inspect --no-notify --respawn --transpile-only ./src/bin/www.ts"
  },
  "keywords": [
    "express",
    "node.js",
    "vscode",
    "npm",
    "eslint",
    "prettier"
  ],
  "author": "Anerco",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.2",
    "express": "^4.17.1",
    "http-errors": "^1.8.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.6",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.12",
    "@types/http-errors": "^1.8.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^9.0.2",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-mocha": "^8.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
