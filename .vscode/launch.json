{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch server debug",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"],
      "protocol": "inspector",
      "port": 9229,
      "restart": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "express-ts:*"
      }
    }
  ]
}
